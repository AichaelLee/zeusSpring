<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lzz.mysqlmapper.JobRepository">
    <resultMap type="com.lzz.mysqlentity.Job" id="job">
      <id property="id" column="id" />
      <result property="name" column="name" />
    </resultMap>
  <select id="findOneById" parameterType="int" resultType="com.lzz.mysqlentity.Job">
    SELECT
    id,
    name
    FROM
    job
    WHERE
    id = #{id}
  </select>

</mapper>